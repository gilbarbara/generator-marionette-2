{
	"name": "<%= _.slugify(appname) %>",
	"version": "0.1.0",
	"dependencies": {},
	"devDependencies": {
		"grunt": "~0.4",
		"grunt-contrib-clean": "~0.5",<% if (options.coffee) { %>
		"grunt-contrib-coffee": "~0.10",<% } %><% if (compassBootstrap) { %>
		"grunt-contrib-compass": "~0.9",<% } %>
		"grunt-contrib-concat": "~0.4",
		"grunt-contrib-connect": "~0.8",
		"grunt-contrib-copy": "~0.5",
		"grunt-contrib-cssmin": "~0.10",
		"grunt-contrib-htmlmin": "~0.3",
		"grunt-contrib-imagemin": "~0.7",<% if (templateFramework === 'mustache') { %>
		"grunt-mustache": "~0.1",<% } else if (templateFramework === 'handlebars') { %>
		"grunt-contrib-handlebars": "~0.8",<% } else { %>
		"grunt-contrib-jst": "~0.6",<% } %>
		"grunt-contrib-jshint": "~0.10",
		"grunt-contrib-uglify": "~0.5",
		"grunt-contrib-watch": "~0.6",<% if (testFramework === 'jasmine') { %>
		"grunt-contrib-jasmine": "~0.6",<% }else{ %>
		"grunt-mocha": "~0.4",<% } %>
		"grunt-open": "~0.2",
		"grunt-rev": "~0.1",
		"grunt-usemin": "~2.3",<% if(includeRequireJS){ %>
		"grunt-bower-requirejs": "~0.11",
		"grunt-requirejs": "~0.4",<% } %>
		"connect-livereload": "~0.4",
		"jshint-stylish": "~0.2",
		"load-grunt-tasks": "~0.6",
		"time-grunt": "~0.3"
	},
	"engines": {
		"node": ">=0.10.0"
	}
}
